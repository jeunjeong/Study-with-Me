generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String   @id @default(uuid())
  email    String   @unique @db.VarChar(50)
  name     String   @db.VarChar(50)
  provider Provider
}

enum Provider {
  kakao
  github
}
